<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_search_artist">

    <fragment
        android:id="@+id/navigation_search_artist"
        android:name="com.example.spotifyassignment.ui.fragment.SearchArtistFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_search_artist" />

    <fragment
        android:id="@+id/navigation_search_album"
        android:name="com.example.spotifyassignment.ui.fragment.SearchAlbumFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_search_album" />

    <fragment
        android:id="@+id/navigation_search_audio_book"
        android:name="com.example.spotifyassignment.ui.fragment.SearchAudioBookFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_search_audio_book" />
